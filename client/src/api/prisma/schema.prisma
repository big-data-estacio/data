// schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Bebida {
  id                Int     @id @default(autoincrement())
  nome              String
  preco             Float
  quantidade        Int
  descricao         String?
  total_vendas      Float?
  quantidade_vendas Int?
}
